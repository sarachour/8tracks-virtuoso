{
  "manifest_version": 2,

  "name": "8-Tracks Scrobbler",
  "description": "scobble tracks from 8tracks to spotify.",
  "version": "1.0",

  "permissions": [
	  "storage"
	],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "scrobbler.html"
  },
  "background": {
    "scripts": ["musicplayer.js"],
    "persistent": true
  }
}
